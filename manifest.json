{
  "manifest_version": 3,
  "name": "ChatGPT DOM Cleaner",
  "version": "0.1",
  "description": "Removes old messages to reduce lag",
  "content_scripts": [
    {
      "matches": ["https://chatgpt.com/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
